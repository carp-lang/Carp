(system-include "carp_double.h")

(defmodule Double
  (Numeric.register Double)
  (Floating.register Double)

  (doc zero "returns `0.0`.")
  (defn zero []
    0.0)
)

(defmodule DoubleRef
  (defn = [a b]
    (Double.= @a @b))

  (defn < [a b]
    (Double.< @a @b))

  (defn > [a b]
    (Double.> @a @b))

  (defn + [a b]
    (Double.+ @a @b))
)
