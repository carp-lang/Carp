(Debug.sanitize-addresses)
(Project.config "print-ast" true)

(doc f "This function will return 123.")
(sig f (Fn [] Int))
(defn f [] 123)

(defmodule Foo
  (doc f "This is an evil function.")
  (defn f [] 666)
  (defn g [] 10000))

(doc Foo.g "The 10000 dollar function, annotated from afar.")

(save-docs String)
(save-docs Int)
