(deftype T [x Int y Int z Int])
(derive T inc)
(derive T dec)
(derive-eq T)
(derive-zero T)
(derive-str T)

(defn main []
(do
  (println* &(T.inc (T.init 1 2 3)))
  (println* &(T.dec (T.init 1 2 3)))
  (println* &(T.zero))
  (println* (= &(T.init 1 2 3) &(T.init 1 2 3)))
  (println* (= &(T.init 1 2 3) &(T.init 1 3 2)))))
